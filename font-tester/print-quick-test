#!/usr/bin/env bash

# Sample texts to print to test the font

# Alphabet
alphabet="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
echo $alphabet

# Numbers
numbers="0123456789"
echo $numbers

# Special characters
special_characters="!@#$%^&*()_+-=[]{}|;':\",.\/<>?"
echo $special_characters

# Ligatures symbols
ligatures="-> <- --> <-- => <= == === != !==
            =>> <<= <<= >>= >>>= >>> ===>
            <== <=== <===> <==== <====> <=====>
            <$> <*> <|> </> |> <!-- <~> <~~> <~ ~> <~~ ~> <~>~>"
echo $ligatures

# Greek alphabet
greek_alphabet="Î±Î²Î³Î´ÎµÎ¶Î·Î¸Î¹ÎºÎ»Î¼Î½Î¾Î¿Ï€ÏÏƒÏ„Ï…Ï†Ï‡ÏˆÏ‰"
echo $greek_alphabet

